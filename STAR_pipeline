

mkdir 01.Fastqc
fastqc -o 01.Fastqc/ 00.Rawdata/20200925_RNA_MDS_HA/GR-1448093_JJB_S4_R1_001.fastq.gz 00.Rawdata/20200925_RNA_MDS_HA/GR-1448093_JJB_S4_R2_001.fastq.gz 

mkdir 02.Cleandata
mkdir 02-1.Unpaired
sickle pe -f 00.Rawdata/20200925_RNA_MDS_HA/GR-1448093_JJB_S4_R1_001.fastq.gz -r 00.Rawdata/20200925_RNA_MDS_HA/GR-1448093_JJB_S4_R2_001.fastq.gz -t sanger -o 02.Cleandata/trimmed_GR-1448093_JJB_S4_R1_001.trimmed.fastq.gz -p 02.Cleandata/trimmed_GR-1448093_JJB_S4_R2_001.trimmed.fastq.gz -s 02-1.Unpaired/Unpaired_GR-1448093_JJB_S4_001.fastq.gz -q 20 -l 20























